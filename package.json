{
  "name": "getup",
  "version": "0.5.0",
  "description": "Frontend boilerplate",
  "author": "Genki Katsutani",
  "scripts": {
    "rollup": "rollup -c",
    "js-vendor": "rollup -c rollup.config.vendor.js",
    "css-sass": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/css/main.css dist/main.css",
    "css-postcss": "postcss --config postcss.config.js --replace dist/css/*.css",
    "css": "npm-run-all node-sass node-postcss",
    "html": "ejs-cli --base-dir src/ejs '*.ejs' --out dist/",
    "watch-js": "onchange src/js -- yarn run webpack",
    "watch-css": "onchange src/css -- yarn run css",
    "watch-html": "onchange src/ejs -- yarn run html",
    "watch-sketch": "onchange src/symbol-font-14px.sketch -- gulp sketch",
    "watch-iconfont": "onchange src/iconfont -- gulp iconfont",
    "watch": "npm-run-all --parallel watch-*",
    "min-css": "postcss dist/css/main.css -e minify -o dist/css/main.css",
    "min-image": "imagemin dist/img/*.{jpg,png} --out-dir=dist/img",
    "min": "npm-run-all --parallel min-image",
    "copy-font": "cpx \"node_modules/slick-carousel/slick/fonts/**\" dist/css/fonts",
    "copy-img": "cpx \"node_modules/slick-carousel/slick/ajax-loader.gif\" dist/css",
    "copy-bootstrap": "cpx \"node_modules/bootstrap/scss/**\" src/scss",
    "copy": "npm-run-all copy-*",
    "concat": "concat -o dist/js/vendor.js node_modules/jquery/dist/jquery.min.js node_modules/riot/riot.min.js node_modules/animejs/anime.min.js node_modules/whatwg-fetch/fetch.js node_modules/feature.js/feature.min.js node_modules/p5/lib/p5.min.js node_modules/promise-polyfill/promise.min.js node_modules/scrollmagic/scrollmagic/minified/ScrollMagic.min.js node_modules/scrollmagic/scrollmagic/minified/plugins/debug.addIndicators.min.js node_modules/slick-carousel/slick/slick.min.js",
    "webpack": "webpack --display-error-details"
  },
  "dependencies": {
    "animejs": "^2.0.2",
    "bootstrap": "4.0.0-beta",
    "feature.js": "^1.0.0",
    "flexibility": "^2.0.1",
    "jquery": "^3.2.1",
    "mobile-detect": "^1.3.6",
    "p5": "^0.5.14",
    "popper.js": "^1.12.5",
    "promise-polyfill": "^6.0.2",
    "riot": "^3.5.1",
    "sanitize.css": "^5.0.0",
    "scrollmagic": "^2.0.5",
    "slick-carousel": "^1.7.1",
    "vue": "^2.4.4",
    "whatwg-fetch": "^2.0.3"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nibushibu/Getup.git"
  },
  "bugs": {
    "url": "https://github.com/nibushibu/Getup/issues"
  },
  "homepage": "https://github.com/nibushibu/Getup",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-riot": "^1.1.0",
    "buble": "^0.16.0",
    "concat": "^1.0.3",
    "cpx": "^1.5.0",
    "css-mqpacker": "^6.0.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-consolidate": "^0.2.0",
    "gulp-iconfont": "^9.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sketch": "^1.0.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "onchange": "^3.2.1",
    "perfectionist": "^2.4.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-easings": "^1.0.0",
    "postcss-flexibility": "^2.0.0",
    "postcss-focus": "^2.0.0",
    "postcss-import": "^11.0.0",
    "postcss-mixins": "^6.0.1",
    "postcss-style-guide": "^0.14.0",
    "require-dir": "^0.3.1",
    "riot-tag-loader": "^1.0.0",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-riot": "^1.1.0",
    "rollup-plugin-uglify-es": "^0.0.1",
    "rollup-stream": "^1.23.1",
    "run-sequence": "^2.1.0",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0"
  }
}
