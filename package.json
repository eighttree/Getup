    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-riot": "^1.1.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "buble": "^0.16.0",
    "chai": "^4.1.2",
    "chalk": "^2.0.1",
    "chromedriver": "^2.27.2",
    "concat": "^1.0.3",
